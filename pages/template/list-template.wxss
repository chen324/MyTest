.tempContainer {
  display: flex;
  flex-direction: column;
  margin: 10rpx 0;
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
}

.avatar_data {
  padding: 20rpx 5rpx;
}

.avatar_data image {
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
}

.avatar_data text {
  font-size: 32rpx;
  font-weight: 400;
  margin-left: 20rpx;
}

.title {
  font-size: 34rpx;
  font-weight: 700;
  margin: 5rpx;
}

.contentImg {
  width: 100%;
  height: 460rpx;
}

.content {
  text-indent: 2em;
  font-size: 32rpx;
}

.collection_love image{
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}

.collection_love text {
  font-size: 28rpx;
  margin-right: 10rpx;
}